<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="SGP.Components.Notifications" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2012-05.1.7">
  <WorkerRole name="NotificationRole" vmsize="Small">
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="ServiceBusNamespace" />
      <Setting name="Issuer" />
      <Setting name="Key" />
      <Setting name="InputQueueIdentifier" />
      <Setting name="ErrorQueueIdentifier" />
      <Setting name="PubNubPublishKey" />
      <Setting name="PubNubSubscribeKey" />
      <Setting name="PubNubSecretKey" />
      <Setting name="PubNubMessageChannel" />
    </ConfigurationSettings>
  </WorkerRole>
</ServiceDefinition>